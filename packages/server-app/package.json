{
  "name": "@story-teller/server-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "env": "env $(cat $ENV_FILE | sed '/^#/ d')",
    "infrastructure:up": "npm run env -- docker-compose -p $ENV_FILE up -d",
    "infrastructure:ready": "npm run env -- until docker-compose -p $ENV_FILE exec db pg_isready -U postgres",
    "infrastructure:down": "npm run env -- docker-compose -p $ENV_FILE down",
    "infrastructure:implode": "npm run infrastructure:down -- -v",
    "db:build:queries": "PGPORT=25432 PGDATABASE=compup_test PGUSER=compup PGPASSWORD=password  ./node_modules/.bin/pgtyped -c pgtyped.config.json",
    "db:migrate:create": "npm run env -- db-migrate create",
    "db:migrate:up": "npm run env -- db-migrate up",
    "db:migrate:down": "npm run env -- db-migrate down",
    "db:migrate:redo": "npm run db:migrate:down && npm run db:migrate:up",
    "db:migrate:dump": "npm run env -- schemats generate -c postgres://compup:password@localhost:15432/compup_development -s public -o osm.ts -C",
    "start": "npm run infrastructure:up && npm run env -- ts-node-dev --notify=0 --respawn ./src/index.ts",
    "lint": "eslint --ext js,ts --ignore-path .gitignore .",
    "test": "ENV_FILE=.env.test npm run env -- mocha --timeout 10000 -r ts-node/register --exit --watch-extensions ts $(find src -name '*.spec.ts')",
    "tsc": "tsc"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/deep-equal": "^1.0.1",
    "@types/deep-freeze": "^0.1.2",
    "@types/express": "^4.17.0",
    "@types/nodemailer": "^6.2.1",
    "@types/pg": "^7.4.14",
    "@types/pg-query-stream": "^1.0.2",
    "@types/uuid": "^3.4.5",
    "amqplib": "^0.6.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "deep-equal": "^2.0.3",
    "deep-freeze": "0.0.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "fp-ts": "^2.6.1",
    "hamjest": "^3.7.1",
    "js-joda": "^1.10.1",
    "monocle-ts": "^2.1.0",
    "nodemailer": "^6.3.0",
    "pg": "^7.11.0",
    "pg-camelcase": "0.0.3",
    "pg-query-stream": "^2.0.0",
    "snake-case": "^2.1.0",
    "space-lift": "^0.8.7",
    "sql-template-strings": "^2.2.2",
    "sql-template-tag": "^3.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@pgtyped/cli": "^0.7.3",
    "@story-teller/shared": "*",
    "@types/expect": "^1.20.4",
    "@types/mocha": "^5.2.7",
    "@types/mockdate": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "eslint": "^6.1.0",
    "eslint-config-standard-with-typescript": "^7.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "factory.ts": "^0.5.0",
    "mocha": "^6.1.4",
    "mockdate": "^2.0.5",
    "pre-commit": "^1.2.2",
    "schemats": "^3.0.3",
    "standard": "^14.1.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "ts-sinon": "^1.0.17",
    "typescript": "^3.5.1"
  }
}
