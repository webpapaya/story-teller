    "typescript": "^4.1.2",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "env": "env $(cat $ENV_FILE | sed '/^#/ d')",
    "start": "npm run env -- react-scripts start",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "test": "npm run env -- mocha -r ./src/utils/style-mock.js -r regenerator-runtime/runtime.js -r @swc-node/register --exit --extension ts $(find src -name '*.spec.ts')",
    "build": "npm run env -- react-scripts build",
    "lint": "eslint --ext js,ts --ignore-path .gitignore .",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@story-teller/shared": "../shared",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@swc-node/register": "^1.0.2",
    "@testing-library/react": "^11.2.2",
    "@types/mocha": "^8.2.0",
    "@types/sinon": "^7.5.1",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "hamjest": "^3.7.1",
    "mocha": "^8.2.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "sinon": "^8.0.1",
    "source-map-explorer": "^2.1.2",
    "storybook-react-router": "^1.0.8"
  }
