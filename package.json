{
  "name": "typescript-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "infrastructure:up": "env $(cat $ENV_FILE | sed '/^#/ d') docker-compose -p $ENV_FILE up -d",
    "infrastructure:down": "env $(cat $ENV_FILE | sed '/^#/ d') docker-compose -p $ENV_FILE down -v",
    "db:migrate:create": "env $(cat $ENV_FILE | sed '/^#/ d') db-migrate create",
    "db:migrate:up": "env $(cat $ENV_FILE | sed '/^#/ d') db-migrate up",
    "db:migrate:down": "env $(cat $ENV_FILE | sed '/^#/ d') db-migrate down",

    "start": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "lint": "eslint --ext js,ts .",
    "test": "mocha -r ts-node/register --timeout 500000 --inspect-brk --watch-extensions ts $(find src -name '*.spec.ts')",
    "db:migrate": "docker-compose run flyway -url=jdbc:postgresql://db:5432/compup -user=dbuser -password=password migrate",
    "tsc": "tsc"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/pg": "^7.4.14",
    "@types/pg-query-stream": "^1.0.2",
    "@types/uuid": "^3.4.5",
    "dotenv": "^8.0.0",
    "hamjest": "^3.2.5",
    "js-joda": "^1.10.1",
    "pg": "^7.11.0",
    "pg-camelcase": "0.0.3",
    "pg-query-stream": "^2.0.0",
    "sql-template-tag": "^3.2.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.5.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/expect": "^1.20.4",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "eslint": "^6.1.0",
    "eslint-config-standard-with-typescript": "^7.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^24.8.0",
    "factory.ts": "^0.5.0",
    "mocha": "^6.1.4",
    "pre-commit": "^1.2.2"
  }
}
